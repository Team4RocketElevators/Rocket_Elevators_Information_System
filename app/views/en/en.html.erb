			function triggeredKey(e) {
				var pattern=/^[0-9-]+$/g;
				if(pattern.test(e)){
					document.getElementById("contact:phone").value = e;
				}
				else{
					var txt = e.slice(0, -1);
					document.getElementById("contact:phone").value = txt;
				}
			}
			function switchLanguage(value) {
				document.cookie = value;
				if (value === "FR") {
					var link = "./fr";
					if (document.location.href.includes("#")) {
						var ensublink = document.location.href.split("#")[1];
						var frsublink;
						switch(ensublink) {
							case "awards":
								frsublink = "attributions";
							break;
							case "news":
								frsublink = "actualites";
							break;
							default:
								frsublink = ensublink;
							break;
						}
						if (frsublink != "#") link += "#" + frsublink;
					}
					document.location.href = link;
					console.log(ensublink);
				}else return;
			}
